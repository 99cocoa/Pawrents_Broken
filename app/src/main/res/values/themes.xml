<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.Pawrents" parent="Theme.MaterialComponents.Light.NoActionBar">
    <item name="colorPrimary">@color/purple_grey80</item>
    <item name="colorPrimaryVariant">@color/purple_80</item>
    <item name="colorOnPrimary">@color/white</item>
    <!-- Secondary brand color. -->
    <item name="colorSecondary">@color/pink_80</item>
    <item name="colorSecondaryVariant">@color/pink_80</item>
    <item name="colorOnSecondary">@color/black</item>
    </style>

    <style name="Theme.Pawrents.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Pawrents.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Pawrents.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
    <style name="AppTheme" parent="Theme.MaterialComponents.Light.NoActionBar">
        <!-- Your other theme customizations here -->
    </style>

</resources>